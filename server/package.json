{
  "name": "react-express-project-starter-server",
  "version": "1.0.0",
  "description": "A starter project for running React and Express together",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=local nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage"
  },
  "repository": {},
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-async-wrap": "^1.0.0",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.0.0",
    "mongoose-unique-validator": "^2.0.0",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "nodemailer-mailgun-transport": "^1.3.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.83.0",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/mongoose": "^4.7.33",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "jest": "^22.1.4",
    "mongodb-memory-server": "^1.6.4",
    "mongoose-fixture-loader": "^1.0.2",
    "prettier": "^1.11.1",
    "sinon": "^4.2.1",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -50
      }
    }
  }
}
